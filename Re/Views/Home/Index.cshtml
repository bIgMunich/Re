@model  Common.ObjEntity
@using Models;
@using Common;
<script src="~/Scripts/jquery-1.9.0.min.js"></script>
<link href="~/Content/bootstrap.min.css" rel="stylesheet" />
<script src="~/Scripts/App/Pager.js"></script>
<div class="container">
    <button type="button" id="urls" class="btn-group-vertical">Ulr</button>
    <div class="row"><input id="btnAdd" type="button" value="Add" class="btn-success" /></div>
    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>StatrTime</th>
                <th>Introduce</th>
                <th>IsTrue</th>
            </tr>
        </thead>
        <tbody>
            @if (Model != null)
            {
                List<munich_re> lists = Model.ResultData as List<munich_re>;
                if (lists != null && lists.Count > 0)
                {
                    foreach (var item in lists)
                    {
                        <tr>
                            <td>@item.Id</td>
                            <td>@item.Name</td>
                            <td>@item.StartTime</td>
                            <td>@item.Introduce</td>
                            <td>@item.IsTrue</td>
                        </tr>
                    }
                }
            }
            @{PagerInfo pager = Model.ResultPager as PagerInfo;}
        </tbody>
    </table>
    @Html.ShowPageNavigate((PagerInfo)Model.ResultPager)
    <div>@Html.Raw("当前第" + pager.CurrenetPageIndex + "页") </div>
</div>
<script type="text/javascript">
    document.getElementById("urls").addEventListener("click", function () {
        window.location.href = "/Home/Ulr";
    })
</script>